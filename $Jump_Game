class Solution {
public:
    bool canJump(int A[], int n) {
         int ret = 0;
        int last = 0;
        int curr = 0;
        for (int i = 0; i < n; ++i) {
            if (i > last) {
                if(last == curr) return false;
                last = curr;
                ++ret;
            }
            curr = max(curr, i+A[i]);
        }
        if(last >= n-1) return true;
        else return false; 
    }
}; 

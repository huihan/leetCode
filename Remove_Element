public class Solution {
    public int removeElement(int[] A, int elem) {
        // Start typing your Java solution below
        // DO NOT write main() function
        //if (A.length == 0) return 0;
        
        int a=0;
        for (int b=0; b<A.length; b++) {
            if (A[b] != elem) {
                A[a++] = A[b];
            }
        }
        
        return a;
    }
}
